{
  "manifest_version": 2,
  "name": "Highlight Searcher",
  "version": "1.0",
  "description": "Hackathon project",
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "icon-bitty.png",
    "48": "icon-small.png",
    "128": "icon-large.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["./main.js"]
    }
  ]
}